{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "lib": ["ESNext", "DOM"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "yun-elp": ["../packages/elp"],
      "~/*": ["./.vitepress/vitepress/*"]
    }
  },
  "include": ["**/*", "**/*.vue", ".vitepress/**/*"],
  "exclude": ["node_modules"]
}
