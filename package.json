{
  "name": "yun-elp",
  "version": "1.0.0",
  "description": "基于 Element Plus 的业务组件库",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm && npm config set registry http://mirrors.cloud.tencent.com/npm/",
    "bootstrap": "pnpm install",
    "create": "tsx scripts/create-component.ts",
    "dev": "pnpm -C play dev",
    "docs:dev": "pnpm -C docs dev",
    "test": "pnpm -C packages/components test:coverage",
    "commit": "czg",
    "build": "pnpm build:clean && pnpm build:comp && pnpm build:style && pnpm build:resolver && pnpm build:mcp && pnpm build:pkg && pnpm build:web-types && pnpm build:tags",
    "build:clean": "rimraf dist",
    "build:comp": "pnpm -C packages/components build",
    "build:style": "pnpm -C packages/theme-chalk build",
    "build:resolver": "pnpm -C packages/resolver build",
    "build:pkg": "tsx scripts/copy-pkg.ts",
    "build:docs": "pnpm -C docs build",
    "build:web-types": "tsx scripts/generate-web-types.ts",
    "build:tags": "tsx scripts/generate-tags.ts",
    "build:mcp": "pnpm -C packages/mcp-server build",
    "lint": "pnpm lint:format && pnpm lint:eslint && pnpm lint:style",
    "lint:format": "prettier --write .",
    "lint:eslint": "eslint . --fix",
    "lint:style": "stylelint \"**/*.{vue,scss,css}\" --fix",
    "release": "release-it --config release-it.config.cjs",
    "pre-release": "node scripts/pre-publish-check.cjs",
    "prepare": "husky install",
    "publish": "cd dist && npm publish --registry=https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@yun-elp/components": "workspace:*",
    "@yun-elp/resolver": "workspace:*",
    "@yun-elp/theme-chalk": "workspace:*",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@element-plus/icons-vue": "^2.3.2",
    "@eslint/js": "^9.39.1",
    "@release-it/conventional-changelog": "^8.0.2",
    "@rollup/rollup-win32-x64-msvc": "^4.53.3",
    "@types/glob": "^8.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.14",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/shared": "^3.5.25",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^13.9.0",
    "@vueuse/shared": "^13.9.0",
    "cron-parser": "^5.4.0",
    "cz-git": "^1.12.0",
    "czg": "^1.12.0",
    "echarts": "^6.0.0",
    "element-plus": "^2.11.9",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "glob": "^11.1.0",
    "globals": "^16.5.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "inquirer": "^12.11.1",
    "lodash-es": "^4.17.21",
    "prettier": "^3.7.3",
    "release-it": "^17.11.0",
    "rimraf": "^5.0.10",
    "stylelint": "^16.26.1",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.14",
    "vue": "^3.5.25",
    "vue-eslint-parser": "^10.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  }
}
