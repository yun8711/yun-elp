{
  "name": "yun-elp",
  "version": "1.0.0",
  "description": "基于 Element Plus 的业务组件库",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./es/*.mjs": {
      "types": "./es/*.d.ts",
      "import": "./es/*.mjs"
    },
    "./es/*": {
      "types": [
        "./es/*.d.ts",
        "./es/*/index.d.ts"
      ],
      "import": "./es/*.mjs"
    },
    "./style": "./theme-chalk/index.css",
    "./themes/*": {
      "import": "./theme-chalk/src/themes/*"
    },
    "./theme-chalk/*": {
      "import": "./theme-chalk/*"
    },
    "./locale": {
      "import": "./es/locale/index.mjs"
    },
    "./locale/*": {
      "import": "./es/locale/lang/*.mjs"
    },
    "./hooks": {
      "import": "./es/hooks/index.mjs"
    },
    "./hooks/*": {
      "import": "./es/hooks/*.mjs"
    },
    "./resolver": {
      "types": "./resolver.d.ts",
      "import": "./resolver.mjs"
    },
    "./mcp-server": {
      "import": "./mcp-server/src/index.js",
      "types": "./mcp-server/index.d.ts"
    },
    "./*": "./*"
  },
  "keywords": [
    "vue3",
    "element-plus",
    "component",
    "typescript"
  ],
  "peerDependencies": {
    "element-plus": "^2.10.2",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1",
    "lodash-es": "^4.17.21"
  },
  "optionalDependencies": {
    "cron-parser": "^5.3.1",
    "echarts": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/element-plus/element-plus.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "style": "theme-chalk/index.css",
  "sideEffects": [
    "theme-chalk/**/*.css",
    "theme-chalk/src/**/*.scss"
  ],
  "vetur": {
    "tags": "tags.json"
  },
  "contributes": {
    "html": {
      "customData": "tags.json"
    }
  },
  "web-types": "web-types.json",
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
