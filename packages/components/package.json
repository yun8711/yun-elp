{
  "name": "@kd-elp/components",
  "version": "1.0.0",
  "description": "基于 Element Plus 的业务组件库",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./dist/style.css": "./src/styles/index.scss",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "web-types": "./dist/web-types.json",
  "scripts": {
    "build": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "vue3",
    "component",
    "element-plus",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "element-plus": "^2.4.1",
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@kd-elp/utils": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "@testing-library/vue": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "element-plus": "^2.6.0",
    "happy-dom": "^13.3.8",
    "sass": "^1.0.0",
    "typescript": "^5.4.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  }
}
